// 动画控制变量
bool currentFrame = 0; // 0:正面, 1:侧面
unsigned long lastFrameTime = 0;
const unsigned long FRAME_DELAY = 0.01; // 帧延迟(毫秒)

void setup() {
  // 初始化背光控制
  pinMode(TFT_BL, OUTPUT);
  digitalWrite(TFT_BL, HIGH);
  
  // 初始化TFT屏幕
  tft.initR(INITR_BLACKTAB);
  tft.setRotation(0); // 根据您的屏幕方向调整
  tft.fillScreen(BLACK);
  
  Serial.begin(9600);
  Serial.println("太空人动画启动");
}

void loop() {
  // 检查是否到了切换帧的时间
  if (millis() - lastFrameTime >= FRAME_DELAY) {
    // 清除屏幕
    tft.fillScreen(BLACK);
    
    // 显示当前帧
    if (currentFrame == 0) {
      // 显示正面太空人
      tft.drawBitmap(0, 0, ZM_image, 128, 128, WHITE);
      Serial.println("显示正面太空人");
    } else {
      // 显示侧面太空人
      tft.drawBitmap(0, 0, CM_image, 128, 128, WHITE);
      Serial.println("显示侧面太空人");
    }
    
    // 切换帧
    currentFrame = !currentFrame;
    lastFrameTime = millis();
  }
  
}

